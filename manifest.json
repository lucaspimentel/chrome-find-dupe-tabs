{
    "manifest_version": 3,
    "name": "Duplicate Tab Finder",
    "version": "1.0",
    "permissions": [
        "tabs"
    ],
    "action": {
        "default_icon": {
            "16": "images/icon16.png",
            "24": "images/icon24.png",
            "32": "images/icon32.png"
        }
    },
    "background": {
        "service_worker": "background.js"
    }
}